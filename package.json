{
  "name": "chessington",
  "version": "1.0.0",
  "description": "Starter project for a chess-based TDD exercise",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "test": "mocha --require ts-node/register \"./tests/**/*.spec.ts\"",
    "build": "npm run clean && npm run babel && tsc && npm run bundle",
    "clean": "del-cli dist/**/* logs/*",
    "bundle": "webpack",
    "babel": "babel ./src/frontend --out-dir dist/frontend --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "chai": "^4.1.2",
    "del-cli": "^1.1.0",
    "mocha": "^4.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "express": "^4.16.1"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SopGui/chessington-ts.git"
  },
  "bugs": {
    "url": "https://github.com/SopGui/chessington-ts/issues"
  },
  "homepage": "https://github.com/SopGui/chessington-ts#readme"
}
